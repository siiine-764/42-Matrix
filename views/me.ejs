<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>KNOX</title>
  <script src="https://cdn.tailwindcss.com"></script>
</head>

<body class="h-screen">
  <nav class="navbar fixed-top bg-body-tertiary" style="background-color: rgb(253, 1, 1);">
    <div class="container-fluid">
      <img class="rounded w-96 border border-black" src="<%= data.image.link %>" alt="profile-image" style="width: 25px;">

    </div>
  </nav>


  <div class="w-full flex justify-center items-center h-full">
    


    <div class="p-4 border border-black rounded shadow-xl">
        <a href="./" class="flex hover:underline">
            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" style="fill: rgba(0, 0, 0, 1);"><path d="M21 11H6.414l5.293-5.293-1.414-1.414L2.586 12l7.707 7.707 1.414-1.414L6.414 13H21z"></path></svg>
            <div class="flex px-1"></div>
            <span class="font-mono text-gray-800"><%= data.login %></span>
        </a>
        <div class="flex py-2"></div><hr><div class="flex py-2"></div>
        <img class="rounded w-96 border border-black" src="<%= data.image.link %>" alt="profile-image">
        <div class="flex py-2"></div><hr><div class="flex py-2"></div>
        <div class="text-sm">
            <% Object.entries(data).forEach(([key, value]) => { %>
                <div>
                  
                <% if (key != 'anonymize_date' && key != 'data_erasure_date') { %>
                    <% if (typeof value === 'string') { %>
                      <p class="font-mono"><strong class=""><%= key %>:</strong> <%= value %></p>
                    <% } %>
                <% } %>
                </div>
            <% }); %>
        </div>
    </div>
</div>

</body>
</html>